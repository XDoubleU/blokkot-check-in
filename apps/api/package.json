{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "prod": "node dist/main --max-old-space-size=200",
    "db:migration-create": "mikro-orm migration:create",
    "db:test": "mikro-orm migration:fresh --seed",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "lint:report": "eslint . --output-file eslint_report.json --format json src",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/cli-config.ts",
      "./dist/cli-config.js"
    ]
  },
  "dependencies": {
    "@mikro-orm/core": "5.7.8",
    "@mikro-orm/migrations": "5.7.8",
    "@mikro-orm/nestjs": "5.1.8",
    "@mikro-orm/postgresql": "5.7.8",
    "@mikro-orm/reflection": "5.7.8",
    "@mikro-orm/seeder": "5.7.8",
    "@nestjs/cli": "9.5.0",
    "@nestjs/common": "9.4.2",
    "@nestjs/core": "9.4.2",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.4.2",
    "@nestjs/platform-ws": "9.4.2",
    "@nestjs/throttler": "4.0.0",
    "@nestjs/websockets": "9.4.2",
    "@sentry/node": "7.52.1",
    "@sentry/tracing": "7.52.1",
    "@types/bcrypt": "5.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/json2csv": "5.0.3",
    "@types/passport-jwt": "3.0.8",
    "@types/uuid": "9.0.1",
    "@types/ws": "8.5.4",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "1.4.6",
    "date-fns": "2.30.0",
    "helmet": "7.0.0",
    "inquirer": "9.2.6",
    "json2csv": " 6.0.0-alpha.2",
    "nest-raven": "9.2.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "uuid": "9.0.0",
    "ws": "8.13.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "5.7.8",
    "@nestjs/testing": "9.4.2",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.16",
    "@types/supertest": "2.0.12",
    "eslint": "8.41.0",
    "eslint-config-custom": "1.0.0",
    "jest": "29.5.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tsconfig-custom": "1.0.0",
    "types-custom": "1.0.0",
    "typescript": "5.0.4"
  }
}
