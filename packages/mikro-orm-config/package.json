{
  "name": "mikro-orm-config",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "lint:report": "eslint . --output-file eslint_report.json --format json src",
    "db:seed": "npx mikro-orm seeder:run",
    "db:migration-create": "npx mikro-orm migration:create",
    "db:migration-up": "npx mikro-orm migration:up",
    "db:migration-down": "npx mikro-orm migration:down",
    "db:test": "npx mikro-orm migration:fresh --seed"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/index.ts",
      "./dist/src/index.js"
    ]
  },
  "dependencies": {
    "@mikro-orm/cli": "^5.6.13",
    "@mikro-orm/core": "^5.6.13",
    "@mikro-orm/migrations": "^5.6.13",
    "@mikro-orm/reflection": "^5.6.13",
    "@mikro-orm/seeder": "^5.6.13",
    "@types/bcrypt": "^5.0.0",
    "@types/uuid": "^9.0.1",
    "bcrypt": "^5.1.0",
    "eslint": "^8.35.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "eslint-config-custom": "*",
    "tsconfig-custom": "*",
    "types-custom": "*"
  }
}