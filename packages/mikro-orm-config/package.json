{
  "name": "mikro-orm-config",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "lint:report": "eslint . --output-file eslint_report.json --format json src",
    "db:migration-create": "mikro-orm migration:create",
    "db:test": "mikro-orm migration:fresh --seed"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/cli-config.ts",
      "./dist/cli-config.js"
    ]
  },
  "dependencies": {
    "@mikro-orm/core": "5.6.16",
    "@mikro-orm/migrations": "5.6.16",
    "@mikro-orm/reflection": "5.6.16",
    "@mikro-orm/seeder": "5.6.16",
    "bcrypt": "5.1.0",
    "ts-node": "10.9.1",
    "types-custom": "1.0.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "5.6.16",
    "@types/bcrypt": "5.0.0",
    "@types/node": "18.15.13",
    "@types/uuid": "9.0.1",
    "eslint": "8.39.0",
    "eslint-config-custom": "1.0.0",
    "tsconfig-custom": "1.0.0",
    "typescript": "5.0.4"
  }
}
