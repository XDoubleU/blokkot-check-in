{
  "name": "mikro-orm-config",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "lint:report": "eslint . --output-file eslint_report.json --format json src",
    "db:migration-create": "mikro-orm migration:create",
    "db:test": "mikro-orm migration:fresh --seed"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/cli-config.ts",
      "./dist/src/cli-config.js"
    ]
  },
  "dependencies": {
    "@mikro-orm/core": "^5.6.15",
    "@mikro-orm/migrations": "^5.6.15",
    "@mikro-orm/reflection": "^5.6.15",
    "@mikro-orm/seeder": "^5.6.15",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.15.10",
    "@types/uuid": "^9.0.1",
    "bcrypt": "^5.1.0",
    "eslint": "^8.36.0",
    "eslint-config-custom": "*",
    "ts-node": "^10.9.1",
    "tsconfig-custom": "*",
    "types-custom": "*",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.6.15"
  }
}
