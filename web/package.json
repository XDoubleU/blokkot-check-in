{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "analyze": "cross-env ANALYZE=true next build",
    "format": "prettier --write ./**/*.{ts,tsx}",
    "lint": "eslint . && tsc --noEmit && jscpd . && knip && prettier --check ./**/*.{ts,tsx} && npmPkgJsonLint .",
    "lint:fix": "eslint . --fix",
    "swag": "npx openapi-typescript@5.4 ./../api/docs/swagger.json -o ./src/api-wrapper/types/schema.d.ts --export-type",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@mdx-js/loader": "3.0.0",
    "@mdx-js/react": "3.0.0",
    "@next/mdx": "14.0.4",
    "@popperjs/core": "2.11.8",
    "@sentry/nextjs": "7.92.0",
    "bootstrap": "5.3.2",
    "bootstrap-icons": "1.11.3",
    "moment": "2.30.1",
    "moment-timezone": "0.5.44",
    "next": "14.0.4",
    "next-pwa": "5.6.0",
    "prop-types": "15.8.1",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-bootstrap": "2.9.2",
    "react-dom": "18.2.0",
    "react-hook-form": "7.49.2",
    "react-spinners": "0.13.8",
    "recharts": "2.10.3",
    "sass": "1.69.7",
    "typescript": "5.3.3",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.0.4",
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.7",
    "@types/react": "18.2.47",
    "@types/testing-library__jest-dom": "5.14.9",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-redundant-undefined": "1.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-testing-library": "6.2.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-mock": "29.7.0",
    "jest-websocket-mock": "2.5.0",
    "jscpd": "3.5.10",
    "knip": "3.12.0",
    "next-router-mock": "0.9.11",
    "npm-package-json-lint": "7.1.0",
    "openapi-typescript": "6.7.3",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1"
  },
  "engines": {
    "node": "20.10.x",
    "yarn": "1.x"
  },
  "npmpackagejsonlint": {
    "rules": {
      "prefer-absolute-version-dependencies": "error",
      "prefer-alphabetical-dependencies": "error",
      "prefer-absolute-version-devDependencies": "error",
      "prefer-alphabetical-devDependencies": "error"
    }
  },
  "jscpd": {
    "exitCode": 1,
    "pattern": "**/*.{ts,tsx}",
    "ignore": [
      "**/*.d.ts",
      "**/coverage/**",
      "**/node_modules/**",
      "**/*.test.{ts,tsx}"
    ],
    "format": [
      "typescript",
      "tsx"
    ]
  },
  "knip": {
    "exclude": [
      "classMembers",
      "enumMembers"
    ],
    "ignore": [
      "src/api-wrapper/types/*",
      ".yarn"
    ],
    "ignoreDependencies": [
      "styles",
      "jest-environment-jsdom",
      "@types/jest",
      "eslint-config-next"
    ]
  },
  "prettier": {
    "parser": "typescript",
    "semi": false,
    "trailingComma": "none"
  }
}
