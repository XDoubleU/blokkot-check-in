{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cpx ./serve.json ./out/ && npx serve@latest out",
    "build": "yarn run swag && next build",
    "analyze": "cross-env ANALYZE=true next build",
    "format": "prettier --write ./**/*.{ts,tsx}",
    "lint": "eslint . && tsc --noEmit && jscpd . && knip && prettier --check ./**/*.{ts,tsx} && npmPkgJsonLint .",
    "lint:fix": "eslint . --fix",
    "swag": "npx openapi-typescript@5.4 ./../api/docs/swagger.json -o ./src/api-wrapper/types/schema.d.ts --export-type",
    "test": "jest",
    "test:cov": "jest --coverage",
    "jest-preview": "jest-preview"
  },
  "dependencies": {
    "@mdx-js/loader": "3.0.1",
    "@mdx-js/react": "3.0.1",
    "@next/mdx": "14.2.4",
    "@popperjs/core": "2.11.8",
    "@sentry/nextjs": "8.13.0",
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.11.3",
    "cpx": "^1.5.0",
    "jest-preview": "^0.3.1",
    "moment": "2.30.1",
    "moment-timezone": "0.5.45",
    "next": "14.2.4",
    "next-pwa": "5.6.0",
    "query-string": "9.0.0",
    "react": "18.3.1",
    "react-bootstrap": "2.10.3",
    "react-dom": "18.3.1",
    "react-hook-form": "7.52.0",
    "react-spinners": "0.14.1",
    "recharts": "2.12.7",
    "sass": "1.77.6",
    "typescript": "5.5.2",
    "uglify-js": "^3.19.2",
    "uuid": "10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.2.4",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.5.2",
    "@types/cpx": "^1",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.9",
    "@types/react": "18.3.3",
    "@types/uglify-js": "^3",
    "@types/uuid": "10.0.0",
    "@types/ws": "^8",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.14.1",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-deprecation": "3.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest-dom": "5.4.0",
    "eslint-plugin-redundant-undefined": "1.0.0",
    "eslint-plugin-sonarjs": "1.0.3",
    "eslint-plugin-testing-library": "6.2.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-mock": "29.7.0",
    "jest-websocket-mock": "2.5.0",
    "jscpd": "4.0.4",
    "knip": "5.23.1",
    "next-router-mock": "0.9.13",
    "npm-package-json-lint": "8.0.0",
    "openapi-typescript": "7.0",
    "prettier": "3.3.2",
    "ts-jest": "29.1.5"
  },
  "engines": {
    "node": "21.x",
    "yarn": "4.x"
  },
  "npmpackagejsonlint": {
    "rules": {
      "prefer-absolute-version-dependencies": "error",
      "prefer-alphabetical-dependencies": "error",
      "prefer-absolute-version-devDependencies": "error",
      "prefer-alphabetical-devDependencies": "error"
    }
  },
  "jscpd": {
    "exitCode": 1,
    "pattern": "**/*.{ts,tsx}",
    "ignore": [
      "**/*.d.ts",
      "**/coverage/**",
      "**/node_modules/**",
      "**/*.test.{ts,tsx}"
    ],
    "format": [
      "typescript",
      "tsx"
    ]
  },
  "prettier": {
    "parser": "typescript",
    "semi": false,
    "trailingComma": "none"
  },
  "packageManager": "yarn@4.3.1"
}
